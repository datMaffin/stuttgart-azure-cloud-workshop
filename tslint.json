{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-standard",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "no-console": {
            "severity": "warning"
        },
        "trailing-comma": [
            false
        ],
        "prettier": [
            true,
            ".prettierrc"
        ]
    },
    "rulesDirectory": [
        "tslint-plugin-prettier"
    ]
}